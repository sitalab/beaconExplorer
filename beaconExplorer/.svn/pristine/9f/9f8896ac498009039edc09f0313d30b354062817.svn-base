package aero.developer.beacons.beaconglass.objects;

import java.io.Serializable;

public class User implements Serializable {

	@Override
	public String toString() {
		return "User [firstName=" + firstName + ", lastName=" + lastName
				+ ", airline=" + airline + ", flight=" + flight + "]";
	}

	/**
	 * 
	 */
	private static final long serialVersionUID = -5731048623196665641L;
	private String firstName = null;
	private String lastName = null;
	private String airline = null;
	private String flight = null;

	public User() {
	}

	public User(String firstName, String lastName, String airline, String flight) {
		super();
		this.firstName = firstName;
		this.lastName = lastName;
		this.airline = airline;
		this.flight = flight;
	}

	public String getFirstName() {
		return firstName;
	}

	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	public String getLastName() {
		return lastName;
	}

	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	public String getAirline() {
		return airline;
	}

	public void setAirline(String airline) {
		this.airline = airline;
	}

	public String getFlight() {
		return flight;
	}

	public void setFlight(String flight) {
		this.flight = flight;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	

}
